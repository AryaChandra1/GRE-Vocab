{
    "manifest_version": 3,
    "name": "GRE Vocabulary Extension",
    "version": "1.0",
    "description": "Learn GRE vocabulary with every new tab",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }